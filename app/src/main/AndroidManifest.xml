        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="ZZPZS9SZh5OfwGqUbBz7xOqpOenWLDWE" />
        <activity android:name=".UI.MainActivityLoading">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".UI.CourtActivity"/>
        <activity android:name=".UI.OrderActivity"/>
        <activity android:name=".UI.UserActivity"/>
        <activity android:name=".UI.CourtResultRecycleviewActivity"/>
        <activity android:name=".UI.MapActivity"/>
        <activity android:name=".UI.RegisterActivity"/>

        <service
            android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote">
        </service>
    </application>

</manifest>